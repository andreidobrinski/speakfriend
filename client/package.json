  "scripts": {
    "postinstall": "node_modules/.bin/elm-package install --yes",
    "webpack": "export TMPDIR=/tmp; `npm bin`/webpack",
    "webpack:w": "export TMPDIR=/tmp; `npm bin`/webpack --watch",
    "webpack:dev": "export TMPDIR=/tmp; `npm bin`/webpack-dev-server --host 0.0.0.0 --hot --inline --watch-poll --port 4001",
    "build": "webpack",
    "start": "npm run webpack:dev"
  },
  "devDependencies": {
    "css-loader": "^0.28.1",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
